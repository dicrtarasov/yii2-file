{
	"name" : "dicr/yii2-file",
	"description" : "Компонент для работы с файлами",
	"type" : "yii2-extension",
	"homepage" : "https://gitlab.com/dicr/yii2-file",
	"authors" : [{
			"name" : "Igor Tarasov",
			"email" : "develop@dicr.org",
			"homepage" : "http://dicr.org",
			"role" : "developer"
		}
	],
	"require" : {
		"yiisoft/yii2" : "~2.0",
		"yiisoft/yii2-jui" : "~2.0"
	},
	"repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}
	],
	"keywords" : [
		"yii2 dicr filestore"
	],
	"autoload" : {
		"psr-4" : {
			"dicr\\file\\" : "src/"
		}
	},
	"support" : {
		"source" : "https://gitlab.com/dicr/yii2-file.git"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6",
		"yiisoft/yii2-debug" : "~2.0",
		"yiisoft/yii2-bootstrap" : "~2.0"
	}
}